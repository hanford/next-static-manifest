{
  "version": "0.8.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "next-static-manifest": "./dist/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "preferGlobal": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --entry ./src/index.ts --entry ./src/utils.ts && ./add-node-script",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --entry ./src/index.ts --entry ./src/utils.ts && ./add-node-script",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "next": "^9.5.0 || ^10.0.0 || ^11.0.0 || ^12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "next-static-manifest",
  "author": "Jack Hanford",
  "module": "dist/next-static-manifest.esm.js",
  "size-limit": [
    {
      "path": "dist/next-static-manifest.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/next-static-manifest.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "babel-jest": "^27.3.1",
    "husky": "^4.3.0",
    "next": "^12.0.2",
    "react": "^17.0.2",
    "size-limit": "^4.6.0",
    "tsdx": "https://github.com/agilgur5/tsdx.git#dist-multi",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/yargs": "^15.0.8",
    "yargs": "^16.0.3"
  }
}
